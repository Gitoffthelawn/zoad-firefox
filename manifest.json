{
	"name":"Zoad",
	"short_name":"Zoad",
	"description":"A custom theme loader for your Firefox homepage.",
	"version":"0.0.2",
	"manifest_version": 2,
	"permissions": [
		"theme",
		"activeTab",
		"tabs",
		"search",
		"storage"
	],
	"options_ui":{
		"page":"options/index.html"
	},
	
	"background":{
		"scripts":["background.js"]
	}

}